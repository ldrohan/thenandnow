<h1><%= current_user.email  %>'s Stories</h1>

<% @stories.each do |story| %>
  <h2><%= story.name  %></h2>
    <ul>
      <li><%= image_tag story.photoone, {height: 45} %></li>
      <li><%= image_tag story.phototwo, {height: 45} %></li>
    </ul>  
<% end %>    


  <p><a id="newstorybutton" class="btn btn-primary btn-lg" role="button">Create New Photo Story!</a></p>
